{
  "name": "@kakasoo/github-sdk",
  "version": "0.1.2",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "description": "SDK library generated by Nestia",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "rimraf lib && tsc",
    "build:test": "rimraf bin && tsc --project test/tsconfig.json",
    "deploy": "npm run build && npm publish",
    "dev": "npm run build:test -- --watch",
    "hello": "node hello",
    "prepare": "ts-patch install && typia patch",
    "start": "ts-node test/start.ts",
    "swagger": "ts-node test/swagger.ts",
    "test": "ts-node test/index.ts",
    "test:simulate": "ts-node test/index.ts --simulate true",
    "test:manual": "ts-node test/manual.ts",
    "prettier": "prettier src --write && prettier test --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samchon/nestia"
  },
  "author": "kakasoo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samchon/nestia/issues"
  },
  "homepage": "https://nestia.io",
  "files": [
    "lib",
    "swagger.json",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "@nestia/fetcher": "^3.11.1",
    "tgrid": "^1.0.3",
    "typia": "^6.9.0"
  },
  "devDependencies": {
    "@nestia/e2e": "^0.7.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^4.17.21",
    "@types/inquirer": "9.0.7",
    "@types/swagger-ui-express": "^4.1.6",
    "chalk": "5.3.0",
    "commander": "^12.1.0",
    "express": "^4.19.2",
    "inquirer": "10.1.8",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.21.0",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "typescript": "^5.5.4",
    "typescript-transform-paths": "^3.5.0"
  }
}